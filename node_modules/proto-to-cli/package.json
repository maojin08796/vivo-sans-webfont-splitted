{
  "name": "proto-to-cli",
  "version": "1.2.1",
  "main": "./dist/cli.js",
  "module": "./dist/cli.mjs",
  "description": "A tool to convert Protocol Buffers files to command line interfaces.",
  "bin": {
    "proto-to-cli": "dist/cli.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "protobuf",
    "cli",
    "commander"
  ],
  "author": "",
  "license": "KonghaYao <3446798488@qq.com>",
  "dependencies": {
    "change-case": "^5.4.4",
    "commander": "^13.1.0",
    "fs-extra": "^11.2.0",
    "protobufjs": "^7.4.0"
  },
  "scripts": {
    "test:ts": "./dist/cli.js -i ../../crates/proto/src/index.proto -m InputTemplate -o ./dist/gen/proto.ts",
    "test:rust": "pnpm ./dist/cli.js -i ../../crates/proto/src/index.proto -m InputTemplate -o ../cli/src/proto.rs -t clap",
    "prepublish": "pnpm vite build"
  }
}